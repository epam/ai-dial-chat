{
  "name": "ai-dial-chat-e2e",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/ai-dial-chat-e2e/src",
  "targets": {
    "e2e": {
      "executor": "@nx/playwright:playwright",
      "outputs": ["{workspaceRoot}/dist/.playwright/apps/ai-dial-chat-e2e"],
      "configurations": {
        "local": {
          "config": "apps/ai-dial-chat-e2e/config/local.playwright.config.ts"
        },
        "ci": {
          "config": "apps/ai-dial-chat-e2e/config/playwright.config.ts"
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    }
  },
  "implicitDependencies": ["ai-dial-chat"]
}
