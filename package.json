{
  "name": "dial-chat",
  "version": "0.4.0-rc",
  "private": true,
  "scripts": {
    "postinstall": "node tools/patch-nextjs.js",
    "dev": "next dev",
    "dev:overlay": "esbuild overlay-examples/main.ts --bundle --outdir=overlay-examples --servedir=overlay-examples",
    "build": "npm run build:overlay && next build",
    "build:overlay": "esbuild overlay/index.ts --bundle --format=esm --outfile=public/overlay/script.js",
    "start": "next start",
    "lint": "eslint src && prettier --check src",
    "lint:fix": "eslint src --fix && prettier --write src",
    "format": "prettier --write src",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "coverage:covered": "vitest run --coverage.enabled --coverage.all=false",
    "test:e2e": "playwright test --config=./e2e/config/local.playwright.config.ts",
    "test:e2e:html": "playwright show-report ./e2e/html-report",
    "test:e2e-ci": "playwright test --config=./e2e/config/playwright.config.ts",
    "allure:serve": "allure serve ./e2e/allure-results"
  },
  "dependencies": {
    "@dqbd/tiktoken": "^1.0.7",
    "@floating-ui/react": "^0.25.0",
    "@opentelemetry/exporter-prometheus": "^0.43.0",
    "@opentelemetry/exporter-trace-otlp-proto": "^0.43.0",
    "@opentelemetry/instrumentation": "^0.43.0",
    "@opentelemetry/instrumentation-http": "^0.43.0",
    "@opentelemetry/resources": "^1.17.0",
    "@opentelemetry/sdk-metrics": "^1.17.0",
    "@opentelemetry/sdk-node": "^0.43.0",
    "@opentelemetry/sdk-trace-base": "^1.17.0",
    "@opentelemetry/semantic-conventions": "^1.17.0",
    "@reduxjs/toolkit": "^1.9.5",
    "@tabler/icons-react": "^2.9.0",
    "classnames": "^2.3.2",
    "css.escape": "^1.5.1",
    "downshift": "^8.1.0",
    "eventsource-parser": "^0.1.0",
    "i18next": "^22.4.13",
    "isomorphic-dompurify": "^1.8.0",
    "mime-types": "^2.1.35",
    "next": "13.5.4",
    "next-auth": "^4.24.5",
    "next-i18next": "^13.2.2",
    "node-fetch": "^3.3.2",
    "pino": "^8.15.1",
    "pino-pretty": "^10.2.0",
    "rc-slider": "^10.2.1",
    "re-resizable": "^6.9.11",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hot-toast": "^2.4.0",
    "react-i18next": "^12.2.0",
    "react-inlinesvg": "^4.1.0",
    "react-markdown": "^8.0.5",
    "react-redux": "^8.1.2",
    "react-syntax-highlighter": "^15.5.0",
    "redux-observable": "^2.0.0",
    "rehype-raw": "^6.1.1",
    "rehype-sanitize": "^5.0.1",
    "remark-gfm": "^3.0.1",
    "rxjs": "^7.8.1",
    "svgo": "^3.0.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@mozilla/readability": "^0.4.4",
    "@svgr/webpack": "^8.0.1",
    "@tailwindcss/typography": "^0.5.9",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@types/css.escape": "^1.5.0",
    "@types/jsdom": "^21.1.1",
    "@types/mime-types": "^2.1.4",
    "@types/node": "20.9.0",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@types/react-syntax-highlighter": "^15.5.6",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.0.1",
    "autoprefixer": "^10.4.16",
    "dotenv": "^16.3.1",
    "endent": "^2.1.0",
    "esbuild": "^0.19.3",
    "eslint": "^8.49.0",
    "eslint-config-next": "13.4.19",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "eslint-plugin-testing-library": "^6.1.2",
    "gpt-3-encoder": "^1.1.4",
    "jsdom": "^21.1.1",
    "postcss": "^8.4.21",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2",
    "vitest": "^1.0.1"
  },
  "optionalDependencies": {
    "@playwright/test": "^1.38.0",
    "allure-playwright": "^2.6.0"
  },
  "overrides": {
    "@opentelemetry/exporter-jaeger": "file:dev/null",
    "@babel/traverse": "7.23.2"
  },
  "engines": {
    "node": ">=18.18.2",
    "npm": ">=9.8.1"
  }
}
