{
  "name": "dial-chat",
  "version": "1.9.0",
  "private": true,
  "scripts": {
    "postinstall": "npx playwright install",
    "dev": "next dev",
    "watch:overlay": "tsc overlay/script.ts --watch --target es6 --skipLibCheck --outDir dist/overlay",
    "dev:overlay": "npx cpy-cli ./overlay/example.html ./dist --rename=index.html && npx http-server ./dist/overlay --c-1",
    "dev:overlay-advanced": "npx cpy-cli ./overlay/example-advanced.html ./dist --rename=index.html && npx http-server ./dist/overlay -c-1",
    "build": "npm run build:overlay && next build",
    "build:overlay": "tsc overlay/script.ts --target es6 --skipLibCheck --outDir public/overlay",
    "start": "next start",
    "lint": "eslint src && prettier --check src",
    "lint:fix": "eslint src --fix && prettier --write src",
    "format": "prettier --write src",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "test:e2e": "playwright test --config=./e2e/config/local.playwright.config.ts",
    "test:e2e:html": "playwright show-report ./e2e/html-report",
    "test:e2e-ci": "playwright test --config=./e2e/config/playwright.config.ts",
    "allure:serve": "allure serve ./e2e/allure-results"
  },
  "dependencies": {
    "@aashutoshrathi/word-wrap": "^1.2.6",
    "@dqbd/tiktoken": "^1.0.7",
    "@floating-ui/react": "^0.25.0",
    "@opentelemetry/exporter-prometheus": "^0.41.2",
    "@opentelemetry/exporter-trace-otlp-proto": "^0.41.2",
    "@opentelemetry/instrumentation": "^0.41.2",
    "@opentelemetry/instrumentation-http": "^0.41.2",
    "@opentelemetry/resources": "^1.15.2",
    "@opentelemetry/sdk-metrics": "^1.15.2",
    "@opentelemetry/sdk-node": "^0.41.2",
    "@opentelemetry/sdk-trace-base": "^1.15.2",
    "@opentelemetry/semantic-conventions": "^1.15.2",
    "@reduxjs/toolkit": "^1.9.5",
    "@tabler/icons-react": "^2.9.0",
    "classnames": "^2.3.2",
    "css.escape": "^1.5.1",
    "downshift": "^8.1.0",
    "eventsource-parser": "^0.1.0",
    "i18next": "^22.4.13",
    "isomorphic-dompurify": "^1.8.0",
    "next": "13.4.4",
    "next-auth": "^4.22.1",
    "next-i18next": "^13.2.2",
    "node-fetch": "^3.3.2",
    "pino": "^8.15.1",
    "rc-slider": "^10.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hot-toast": "^2.4.0",
    "react-i18next": "^12.2.0",
    "react-markdown": "^8.0.5",
    "react-redux": "^8.1.2",
    "react-syntax-highlighter": "^15.5.0",
    "redux-observable": "^2.0.0",
    "rehype-raw": "^6.1.1",
    "rehype-sanitize": "^5.0.1",
    "remark-gfm": "^3.0.1",
    "rxjs": "^7.8.1",
    "svgo": "^3.0.2",
    "uuid": "^9.0.0",
    "pino-pretty": "^10.2.0"
  },
  "devDependencies": {
    "@mozilla/readability": "^0.4.4",
    "@playwright/test": "^1.37.1",
    "@svgr/webpack": "^8.0.1",
    "@tailwindcss/typography": "^0.5.9",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/css.escape": "^1.5.0",
    "@types/jsdom": "^21.1.1",
    "@types/node": "18.15.0",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@types/react-syntax-highlighter": "^15.5.6",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "@vitest/coverage-c8": "^0.29.7",
    "allure-playwright": "^2.6.0",
    "autoprefixer": "^10.4.14",
    "dotenv": "^16.3.1",
    "endent": "^2.1.0",
    "eslint": "^8.36.0",
    "eslint-config-next": "13.4.10",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "gpt-3-encoder": "^1.1.4",
    "jsdom": "^21.1.1",
    "postcss": "^8.4.21",
    "prettier": "^2.8.7",
    "prettier-plugin-tailwindcss": "^0.2.5",
    "tailwindcss": "^3.3.3",
    "typescript": "^4.9.5",
    "vitest": "^0.29.7"
  },
  "overrides": {
    "word-wrap": "$@aashutoshrathi/word-wrap"
  }
}
